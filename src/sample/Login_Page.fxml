                  <MenuItem mnemonicParsing="false" text="Select All" />
                  <MenuItem mnemonicParsing="false" text="Unselect All" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="443.0" prefWidth="754.0" style="-fx-background-color: #2d3447;" VBox.vgrow="ALWAYS">
         <children>
            <HBox layoutX="513.0" layoutY="236.0" prefHeight="58.0" prefWidth="386.0">
               <children>
                  <Label prefHeight="58.0" prefWidth="163.0" text="Outback" textFill="#2196f3">
                     <font>
                        <Font size="40.0" />
                     </font>
                  </Label>
                  <Label prefHeight="58.0" prefWidth="232.0" text="Auto Repair" textFill="#a0a2ab">
                     <font>
                        <Font size="40.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <PasswordField fx:id="Login_Password" layoutX="542.0" layoutY="430.0" prefHeight="44.0" prefWidth="290.0" promptText="Password">
               <font>
                  <Font size="15.0" />
               </font></PasswordField>
            <TextField fx:id="Login_Email" layoutX="542.0" layoutY="350.0" prefHeight="44.0" prefWidth="290.0" promptText="Email">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <ImageView fitHeight="102.0" fitWidth="269.0" layoutX="546.0" layoutY="76.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Resources/Car Repair logo.png" />
