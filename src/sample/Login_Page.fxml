<VBox fx:id="Login_Controller_Pane" prefHeight="820.0" prefWidth="1320.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Login_Controller">
   <children>
      <MenuBar VBox.vgrow="NEVER">
